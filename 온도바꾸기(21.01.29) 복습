def fahrenheit_to_celsius(fahrenheit):
    return (fahrenheit - 32) * 5 / 9

temperature_list = [40, 15, 32, 64, -4, 11]
print("화씨 온도 리스트: {}".format(temperature_list))

# 리스트의 값들을 화씨에서 섭씨로 변환하는 코드를 입력하세요.
celsius_list = []
i = 0

while i < len(temperature_list):
    celsius_list.append(round(fahrenheit_to_celsius(temperature_list[i]),1))
    i += 1

temperature_list = celsius_list
print("섭씨 온도 리스트: {}".format(temperature_list)) # 섭씨 온도 출력
